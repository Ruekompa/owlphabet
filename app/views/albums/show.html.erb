 <div class="parallax-container">
    <div class="parallax"><img src="<%= @project.cover_photo_url %>"></div>
 </div>
 <div class="section grey lighten-3">
 <div class="container albums white">
  <div class="row">
  <div class="col s12 m3 l3">
  <img class="responsive-img materialboxed" src="<%= @album.cover_art_url %>">
  </div>
    <div class="col s12 m9 l9">
 	<h1><%= @album.title %></h1>
 	  <% if @tracks == nil %>
      <p>No Tracks Found</p>
      <a href="" class="btn red">Add Tracks</a>
      <% else %>
      <% @track.each do |t| %>
 	  <div class="collection">
 	  <% t.split.each do |s|%>
 	    <a href="#!" class="collection-item">s.title </a>
 	     <% end %>
      </div>
      <% end %>
      <% end %>
 	</div>
 	</div>
 	</div>
 	</div>


 	<script type="text/javascript">
  $(document).ready(function(){
    $('.materialboxed').materialbox();
  });
  </script>

