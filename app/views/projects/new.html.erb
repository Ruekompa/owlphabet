 <%= form_for @project do |f| %>
<section class="section">
<div class="container">
        <div class="col s12 m12">
          <div class="card">
            <div class="card-content">
             <div class="row">
               <div class="file-field input-field">
                 <div class="btn red accent-2">
                 <span>Project Cover</span>
                <%= f.file_field :cover_photo %>
                <i class="material-icons right">image</i>
                 </div>
               </div>
             </div>

           <div class="row">
             <div class="input-field col s12">
            <label for="title" class="grey-text">Title</label>
            <%= f.text_field :title, :class => "validate", :type => "text" %>

            </div>
            </div>
        
          </div>
            <div class="card-action">
              <%= f.submit 'Submit', :class => "btn btn-waves red accent-2" %>
            </div>
          </div>
      
        </div>
      </div>
      <% end %>
</div>
</section>


